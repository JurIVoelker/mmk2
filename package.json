{
  "name": "mmk2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:migrate": "prisma migrate dev --schema ./src/prisma/dev.prisma --name init",
    "db:setup": "npx tsx ./src/prisma/scripts/setup.ts && npm run db:migrate",
    "db:add-test-data": "npx tsx ./src/prisma/scripts/addTestData.ts",
    "prod:migrate": "prisma migrate dev --schema ./src/prisma/prod.prisma",
    "prod:init": "prisma migrate reset --schema ./src/prisma/prod.prisma --force && npm run prod:migrate && npm run minio:export-migrations && npm run build",
    "prod:deploy": "npm run minio:import-migrations && npm run prod:migrate && npm run minio:export-migrations && npm run build",
    "minio:import-migrations": "npx tsx ./src/prisma/scripts/importMigrations.ts",
    "minio:export-migrations": "npx tsx ./src/prisma/scripts/exportMigrations.ts"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@prisma/client": "^6.6.0",
    "@radix-ui/react-label": "^2.1.3",
    "@radix-ui/react-slot": "^1.2.0",
    "@types/archiver": "^6.0.3",
    "@types/unzipper": "^0.10.11",
    "archiver": "^7.0.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.487.0",
    "minio": "^8.0.5",
    "next": "15.3.0",
    "next-themes": "^0.4.6",
    "prisma": "^6.6.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.2.0",
    "tw-animate-css": "^1.2.5",
    "unzipper": "^0.12.3",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "typescript": "^5",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@tailwindcss/postcss": "^4",
    "tailwindcss": "^4",
    "eslint": "^9",
    "eslint-config-next": "15.3.0",
    "@eslint/eslintrc": "^3"
  }
}

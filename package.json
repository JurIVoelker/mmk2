{
  "name": "mmk2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:migrate": "prisma migrate dev --schema ./src/prisma/schema.prisma --name init",
    "db:setup": "npx tsx ./src/prisma/scripts/setup.ts && npm run db:generate-client && npx prisma migrate deploy --schema ./src/prisma/schema.prisma",
    "db:enter-data": "npx tsx ./src/scripts/enter-data-to-db.ts",
    "db:add-test-data": "npx tsx ./src/prisma/scripts/addTestData.ts",
    "db:generate-client": "npx prisma generate --schema src/prisma/schema.prisma ",
    "prod:deploy": "npm run db:setup && npm run db:generate-client && npm run build"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@prisma/client": "^6.6.0",
    "@radix-ui/react-checkbox": "^1.2.3",
    "@radix-ui/react-label": "^2.1.3",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-tabs": "^1.1.9",
    "@types/archiver": "^6.0.3",
    "@types/unzipper": "^0.10.11",
    "archiver": "^7.0.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "framer-motion": "^12.9.0",
    "lucide-react": "^0.487.0",
    "minio": "^8.0.5",
    "motion": "^12.15.0",
    "next": "15.3.0",
    "next-themes": "^0.4.6",
    "node-html-parser": "^7.0.1",
    "prisma": "^6.6.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.2.0",
    "tsx": "^4.19.3",
    "tw-animate-css": "^1.2.5",
    "unzipper": "^0.12.3",
    "uuid": "^11.1.0",
    "zod": "^3.24.2",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
